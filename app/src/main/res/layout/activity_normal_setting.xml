<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg"
    android:orientation="vertical" >


    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/xy40dp"
        android:background="@color/light_gray"
        android:orientation="horizontal" >

        <ImageButton
            android:id="@+id/ib_normal_setting_back"
            android:layout_width="@dimen/xy30dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_margin="@dimen/xy10dp"
            android:background="@null"
            android:clickable="true"
            android:src="@drawable/left_back" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:text="@string/normal_setting"
            android:textColor="@color/black"
            android:textSize="@dimen/xy18sp" />
    </FrameLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/xy1dp"
        android:layout_marginLeft="@dimen/xy10dp"
        android:layout_marginRight="@dimen/xy10dp"
        android:background="@drawable/wear_1" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <LinearLayout
            android:id="@+id/normal_setting_layout"
            android:layout_marginTop="@dimen/xy10dp"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_marginTop="@dimen/xy10dp"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/xy5dp"
                android:layout_marginRight="@dimen/xy5dp"
                android:gravity="center_vertical"
                android:orientation="horizontal" >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/xy18sp"
                    android:text="@string/device_no_txt"/>
                <EditText
                    android:id="@+id/et_normal_setting_device_no"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:drawablePadding="@dimen/xy5dp"
                    android:gravity="center_vertical"
                    android:hint="@string/device_no_hint"
                    android:cursorVisible="true"
                    android:singleLine="true"
                    android:maxLength="5"
                    android:inputType="number"
                    android:textColor="#666666"
                    android:textSize="@dimen/xy18dp" />
            </LinearLayout>

            <LinearLayout
                android:layout_marginTop="@dimen/xy10dp"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/xy5dp"
                android:layout_marginRight="@dimen/xy5dp"
                android:gravity="center_vertical"
                android:orientation="horizontal" >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/xy18sp"
                    android:text="@string/store_name_txt"/>
                <EditText
                    android:id="@+id/et_normal_setting_company_name"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:drawablePadding="@dimen/xy5dp"
                    android:gravity="center_vertical"
                    android:hint="@string/store_name_hint"
                    android:cursorVisible="true"
                    android:singleLine="true"
                    android:textColor="#666666"
                    android:textSize="@dimen/xy18dp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/normal_setting_layout_md5_key"
                android:layout_marginTop="@dimen/xy10dp"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/xy5dp"
                android:layout_marginRight="@dimen/xy5dp"
                android:gravity="center_vertical"
                android:orientation="vertical" >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/xy18sp"
                    android:text="MD5 Keyï¼š"/>
                <EditText
                    android:id="@+id/et_normal_setting_md5_key"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/xy60dp"
                    android:drawablePadding="@dimen/xy5dp"
                    android:gravity="center_vertical"
                    android:hint="MD5 Key"
                    android:cursorVisible="true"
                    android:singleLine="true"
                    android:maxLength="32"
                    android:textColor="#666666"
                    android:textSize="@dimen/xy18dp" />
            </LinearLayout>
            <LinearLayout
                android:layout_marginTop="@dimen/xy10dp"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/xy5dp"
                android:layout_marginRight="@dimen/xy5dp"
                android:gravity="center_vertical"
                android:orientation="horizontal" >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/xy18sp"
                    android:text="@string/select_ticket_over_time"/>
                <EditText
                    android:id="@+id/et_normal_setting_time_out"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:drawablePadding="@dimen/xy5dp"
                    android:gravity="center_vertical"
                    android:cursorVisible="true"
                    android:inputType="number"
                    android:singleLine="true"
                    android:textColor="#666666"
                    android:textSize="@dimen/xy18dp" />
            </LinearLayout>

            <LinearLayout
                android:layout_marginTop="@dimen/xy10dp"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/xy5dp"
                android:layout_marginRight="@dimen/xy5dp"
                android:gravity="center_vertical"
                android:orientation="horizontal" >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/xy18sp"
                    android:text="@string/buy_ticket_over_time"/>
                <EditText
                    android:id="@+id/et_normal_setting_pay_time_out"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:drawablePadding="@dimen/xy5dp"
                    android:gravity="center_vertical"
                    android:cursorVisible="true"
                    android:singleLine="true"
                    android:inputType="number"
                    android:textColor="#666666"
                    android:textSize="@dimen/xy18dp" />
            </LinearLayout>

            <LinearLayout
                android:layout_marginTop="@dimen/xy10dp"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/xy5dp"
                android:layout_marginRight="@dimen/xy5dp"
                android:gravity="center_vertical"
                android:orientation="horizontal" >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/xy18sp"
                    android:text="@string/print_ticket_over_time"/>
                <EditText
                    android:id="@+id/et_normal_setting_print_time_out"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:drawablePadding="@dimen/xy5dp"
                    android:gravity="center_vertical"
                    android:inputType="number"
                    android:cursorVisible="true"
                    android:singleLine="true"
                    android:textColor="#666666"
                    android:textSize="@dimen/xy18dp" />
            </LinearLayout>

            <LinearLayout
                android:layout_marginTop="@dimen/xy10dp"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/xy5dp"
                android:layout_marginRight="@dimen/xy5dp"
                android:gravity="center_vertical"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:text="@string/is_printed_qr_code"
                    android:textSize="@dimen/xy18dp" />

                <RadioGroup
                    android:id="@+id/rgp_print_qrcode"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:orientation="horizontal" >
                    <RadioButton
                        android:id="@+id/rbt_isprinted"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="@dimen/xy10dp"
                        android:checked="true"
                        android:gravity="center"
                        android:text="@string/yes"
                        android:textSize="@dimen/xy18dp"/>

                    <RadioButton
                        android:id="@+id/rbt_isnotprinted"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="@dimen/xy20dp"
                        android:checked="false"
                        android:text="@string/no"
                        android:textSize="@dimen/xy18dp" />
                </RadioGroup>
            </LinearLayout>

            <LinearLayout
                android:layout_marginTop="@dimen/xy10dp"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/xy5dp"
                android:layout_marginRight="@dimen/xy5dp"
                android:gravity="center_vertical"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:text="@string/is_show_ads_view"
                    android:textSize="@dimen/xy18dp" />

                <RadioGroup
                    android:id="@+id/rgp_show_mainview"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:orientation="horizontal" >
                    <RadioButton
                        android:id="@+id/rbt_isshowed"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="@dimen/xy10dp"
                        android:checked="true"
                        android:gravity="center"
                        android:text="@string/yes"
                        android:textSize="@dimen/xy18dp"/>

                    <RadioButton
                        android:id="@+id/rbt_isnotshowed"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="@dimen/xy20dp"
                        android:checked="false"
                        android:text="@string/no"
                        android:textSize="@dimen/xy18dp" />
                </RadioGroup>
            </LinearLayout>

            <LinearLayout
                android:layout_marginTop="@dimen/xy10dp"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/xy5dp"
                android:layout_marginRight="@dimen/xy5dp"
                android:gravity="center_vertical"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:text="@string/is_show_order_number"
                    android:textSize="@dimen/xy18dp" />

                <RadioGroup
                    android:id="@+id/rgp_show_orderNumber"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:orientation="horizontal" >
                    <RadioButton
                        android:id="@+id/rbt_is_showed_orderNumber"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="@dimen/xy10dp"
                        android:checked="true"
                        android:gravity="center"
                        android:text="@string/yes"
                        android:textSize="@dimen/xy18dp"/>

                    <RadioButton
                        android:id="@+id/rbt_isnot_showed_orderNumber"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="@dimen/xy20dp"
                        android:checked="false"
                        android:text="@string/no"
                        android:textSize="@dimen/xy18dp" />
                </RadioGroup>
            </LinearLayout>

            <LinearLayout
                android:layout_marginTop="@dimen/xy10dp"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/xy5dp"
                android:layout_marginRight="@dimen/xy5dp"
                android:gravity="center_vertical"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:text="@string/select_pay_device"
                    android:textSize="@dimen/xy18dp" />

            </LinearLayout>

            <LinearLayout
                android:layout_marginTop="@dimen/xy10dp"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/xy5dp"
                android:layout_marginRight="@dimen/xy5dp"
                android:gravity="center_vertical"
                android:orientation="horizontal" >
                <RadioGroup
                    android:id="@+id/rgp_pay_device"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:orientation="horizontal" >
                    <RadioButton
                        android:id="@+id/rbt_lyy"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="@dimen/xy10dp"
                        android:checked="true"
                        android:gravity="center"
                        android:text="@string/lyy_device"
                        android:textSize="@dimen/xy18dp"/>

                    <RadioButton
                        android:id="@+id/rbt_wmq"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="@dimen/xy20dp"
                        android:checked="false"
                        android:text="@string/wmq_device"
                        android:textSize="@dimen/xy18dp" />
                </RadioGroup>
            </LinearLayout>

            <LinearLayout
                android:layout_marginTop="@dimen/xy10dp"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/xy5dp"
                android:layout_marginRight="@dimen/xy5dp"
                android:gravity="center_vertical"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:text="@string/select_bill_acceptor"
                    android:textSize="@dimen/xy18dp" />

            </LinearLayout>

            <LinearLayout
                android:layout_marginTop="@dimen/xy10dp"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/xy5dp"
                android:layout_marginRight="@dimen/xy5dp"
                android:gravity="center_vertical"
                android:orientation="horizontal" >
                <RadioGroup
                    android:id="@+id/rgp_bill_acceptor"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:orientation="horizontal" >
                    <RadioButton
                        android:id="@+id/rbt_ict"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="@dimen/xy10dp"
                        android:checked="true"
                        android:gravity="center"
                        android:text="ICT"
                        android:textSize="@dimen/xy18dp"/>

                    <RadioButton
                        android:id="@+id/rbt_itl"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="@dimen/xy20dp"
                        android:checked="false"
                        android:text="ITL"
                        android:textSize="@dimen/xy18dp" />
                </RadioGroup>
            </LinearLayout>


            <LinearLayout
                android:layout_marginTop="@dimen/xy10dp"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/xy5dp"
                android:layout_marginRight="@dimen/xy5dp"
                android:gravity="center_vertical"
                android:orientation="vertical" >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/xy18sp"
                    android:text="@string/itl_accept_amount"/>
                <EditText
                    android:id="@+id/et_normal_setting_bill_type"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/xy5dp"
                    android:layout_marginRight="@dimen/xy5dp"
                    android:drawablePadding="@dimen/xy10dp"
                    android:gravity="center_vertical"
                    android:cursorVisible="true"
                    android:inputType="textMultiLine"
                    android:singleLine="true"
                    android:textColor="#666666"
                    android:text="1,5,10,20,50,100"
                    android:textSize="@dimen/xy18dp" />
            </LinearLayout>

            <LinearLayout
                android:layout_marginTop="@dimen/xy10dp"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/xy5dp"
                android:layout_marginRight="@dimen/xy5dp"
                android:gravity="center_vertical"
                android:orientation="horizontal" >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/xy18sp"
                    android:text="@string/start_ticket_number"/>
                <EditText
                    android:id="@+id/et_normal_setting_initial_titcket_number"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:drawablePadding="@dimen/xy5dp"
                    android:gravity="center_vertical"
                    android:inputType="number"
                    android:cursorVisible="true"
                    android:singleLine="true"
                    android:textColor="#666666"
                    android:textSize="@dimen/xy18dp" />
            </LinearLayout>

            <TextView
                android:gravity="center_vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/xy5dp"
                android:layout_marginRight="@dimen/xy5dp"
                android:layout_marginTop="@dimen/xy10dp"
                android:layout_marginBottom="@dimen/xy5dp"
                android:textSize="@dimen/xy18sp"
                android:text="@string/buy_ticket_tips_txt"/>
            <EditText
                android:id="@+id/et_normal_setting_pay_desc"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/xy5dp"
                android:layout_marginRight="@dimen/xy5dp"
                android:drawablePadding="@dimen/xy10dp"
                android:gravity="center_vertical"
                android:hint="@string/buy_ticket_tips_hint"
                android:cursorVisible="true"
                android:inputType="textMultiLine"
                android:singleLine="true"
                android:textColor="#666666"
                android:text="@string/pay_desc"
                android:textSize="@dimen/xy18dp" />

            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/xy10dp"
                android:layout_marginRight="@dimen/xy10dp"
                android:orientation="horizontal" >
            </RelativeLayout>

            <Button
                android:id="@+id/bt_normal_setting_confirm"
                android:layout_width="fill_parent"
                android:layout_height="@dimen/xy40dp"
                android:layout_margin="@dimen/xy20dp"
                android:text="@string/save"
                android:textStyle="bold"
                android:textSize="@dimen/xy16sp" />

        </LinearLayout>
    </ScrollView>
</LinearLayout>